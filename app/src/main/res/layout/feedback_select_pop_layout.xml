<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@color/c50000000"
    android:layout_height="match_parent">
    <ListView
        android:id="@+id/feedback_pop_list"
        android:layout_width="match_parent"
        android:layout_alignBottom="@id/feedback_pop_close_bt"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:background="@drawable/boder_rounded_ffffff_ffffff"
        android:layout_marginBottom="60dp"
        android:scrollbars="none"
        android:divider="@color/c00000000"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/feedback_pop_close_bt"
        android:layout_width="match_parent"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/boder_rounded_6ca8cd_6ca8cd"
        android:textSize="18sp"
        android:textColor="@color/ffffff"
        android:layout_marginBottom="20dp"
        android:text="取 消"
        android:layout_height="45dp" />
</RelativeLayout>